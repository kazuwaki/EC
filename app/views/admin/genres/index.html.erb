<h1>ジャンル一覧・追加</h1>
<%= render "form",genre: @genre %>
<table class="table">
  <thead>
    <tr>
      <th>ジャンル</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
        <td><%= link_to "編集", edit_admin_genre_path(genre) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
